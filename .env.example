# Discord Bot Configuration
DISCORD_BOT_TOKEN=your_discord_bot_token_here
DISCORD_USER_ID=your_discord_user_id_here

# API Security
API_KEY=generate_32_character_secure_key_here
FASTAPI_ENDPOINT=http://localhost:8000

# GitHub Integration
GITHUB_TOKEN=your_github_personal_access_token
GITHUB_REPO=username/repository-name
GITHUB_BRANCH=main

# Optional Configuration
SITE_BASE_URL=https://yoursite.com
LOG_LEVEL=INFO
ENVIRONMENT=development

# Storage Provider Configuration
STORAGE_PROVIDER=azure  # Options: azure, linode

# Linode Object Storage Configuration
LINODE_STORAGE_ACCESS_KEY_ID=your_linode_access_key_id
LINODE_STORAGE_SECRET_ACCESS_KEY=your_linode_secret_access_key
LINODE_STORAGE_ENDPOINT_URL=https://us-east-1.linodeobjects.com
LINODE_STORAGE_BUCKET_NAME=your_bucket_name
LINODE_STORAGE_REGION=us-east-1
LINODE_STORAGE_CUSTOM_DOMAIN=https://cdn.lqdev.tech
ENABLE_LINODE_STORAGE=false

# Discord Bot Development
DISCORD_GUILD_ID=your_test_server_guild_id
